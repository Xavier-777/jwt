<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mei.jwt.mapper.UserMapper">

    <select id="login" resultType="com.mei.jwt.entity.User">
        select u_id as id,username,password
        from user
        where username = #{username} and password = #{password}
    </select>

</mapper>